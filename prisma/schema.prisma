generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id               String   @id @default(cuid())
  discordId         String   @unique
  ownedBlueprintIds Int[]    @default([])
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
}
